package com.experion.entity;

import java.util.Date;
import java.util.Scanner;

public class Product {
	private String productCode,productName,productDescription;
	private double productPrize;
	private Date openDate,valilidyDate,expiryDate;
	private boolean active;
	private int serialNoOrginal;
	private static int serialNo = 0;

	public Product(String productCode, String productName, String productDescription, double productPrize,
			Date openDate, Date valilidyDate, Date expriryDate) {
		this.productCode = productCode;
		this.productName = productName;
		this.productDescription = productDescription;
		this.productPrize = productPrize;
		this.openDate = openDate;
		this.valilidyDate = valilidyDate;
		this.expiryDate = expriryDate;
		//this.active = active;
	}

	public String getProductCode() {
		return productCode;
	}

	public void setProductCode(String productCode) {
		this.productCode = productCode;
	}

	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public String getProductDescription() {
		return productDescription;
	}

	public void setProductDescription(String productDescription) {
		this.productDescription = productDescription;
	}

	public Date getOpenDate() {
		return openDate;
	}

	public void setOpenDate(Date openDate) {
		this.openDate = openDate;
	}

	public double getProductPrize() {
		return productPrize;
	}

	public void setProductPrize(double productPrize) {
		this.productPrize = productPrize;
	}

	public Date getValilidyDate() {
		return valilidyDate;
	}

	public void setValilidyDate(Date valilidyDate) {
		this.valilidyDate = valilidyDate;
	}

	public Date getExpriryDate() {
		return expriryDate;
	}

	public void setExpriryDate(Date expriryDate) {
		this.expriryDate = expriryDate;
	}

	public boolean isActive() {
		return active;
	}

	public void setActive(boolean active) {
		this.active = active;
	}

	public int getSerialNo() {
		return serialNo;
	}

	public void setSerialNo(int serialNo) {
		this.serialNo = serialNo;
	}

	public int getSerialNoOrginal() {
		return serialNoOrginal;
	}

	public void setSerialNoOrginal(int serialNoOrginal) {
		this.serialNoOrginal = serialNoOrginal;
	}
	

}
